{
  "name": "Replace CDN",
  "version": "0.9.0",
  "manifest_version": 2,
  "icons": {
    "16": "img/logos.png",
    "24": "img/logos.png",
    "32": "img/logos.png",
    "48": "img/logos.png",
    "64": "img/logos.png",
    "128": "img/logos.png"
