{
  "name": "Replace CDN",
  "version": "0.9.0",
  "manifest_version": 2,
  "icons": {
    "16": "img/16x16.png",
    "32": "img/32x32.png",
    "64": "img/64x64.png",
    "128": "img/128x128.png"
  },
  "description": "将 Google CDN 替换为国内镜像",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "author": "https://github.com/gkide",
  "homepage_url": "https://github.com/gkide/ReplaceCDN"
}